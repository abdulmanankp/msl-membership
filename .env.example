# Backend API URL for frontend integration
VITE_API_URL=https://api.yourdomain.com
# WhatsApp Cloud API (Facebook) credentials
# Generate an access token from the Meta developers console
WHATSAPP_TOKEN=your_facebook_graph_api_token_here

# WhatsApp Business Account Details
WHATSAPP_BUSINESS_ID=2193069461137001
WHATSAPP_PHONE_NUMBER_ID=your_whatsapp_phone_number_id_here

# Graph API version (optional)
GRAPH_API_VERSION=v17.0

# Default behavior if admin settings file is missing
DEFAULT_WHATSAPP_SEND_ON_APPROVAL=true
DEFAULT_WHATSAPP_ENABLED=true

# Server port
PORT=3001
